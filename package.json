{
  "name": "electerm-web-demo",
  "version": "2.91.16",
  "description": "Running electerm in as web app",
  "main": "src/app/app.js",
  "type": "module",
  "scripts": {
    "b": "npm run compile && npm run pug",
    "start": "NODE_ENV=development node ./build/vite/dev-server.js",
    "clean": "node build/bin/clean",
    "build": "echo \"ok\"",
    "pug": "NODE_ENV=production node build/bin/build-pug.js",
    "s": "node build/bin/test-server",
    "v": "vercel --prod",
    "compile": "node build/bin/build",
    "vb": "cross-env NODE_ENV=production vite build --config ./build/vite/conf.js",
    "install": "node build/bin/install",
    "lint": "./node_modules/.bin/standard --verbose",
    "fix": "./node_modules/.bin/standard --fix",
    "lock": "npm i --package-lock-only",
    "r": "./build/bin/release"
  },
  "license": "MIT",
  "langugeRepo": "https://github.com/electerm/electerm-locales",
  "privacyNoticeLink": "https://github.com/electerm/electerm/wiki/privacy-notice",
  "knownIssuesLink": "https://github.com/electerm/electerm/wiki/Know-issues",
  "sponsorLink": "https://electerm.html5beta.com/sponsor-electerm.html",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electerm/electerm-web.git"
  },
  "author": {
    "name": "ZHAO Xudong",
    "email": "zxdong@gmail.com",
    "url": "https://github.com/zxdong262"
  },
  "bugs": {
    "url": "https://github.com/electerm/electerm-web/issues"
  },
  "homepage": "https://electerm.html5beta.com",
  "releases": "https://github.com/electerm/electerm-web/releases",
  "engines": {
    "node": ">=18.0.0"
  },
  "preferGlobal": true,
  "devDependencies": {
    "@ant-design/icons": "5.6.1",
    "@ctrl/tinycolor": "^4.1.0",
    "@electerm/electerm-locales": "2.0.37",
    "@electerm/electerm-react": "1.91.16",
    "@electerm/electerm-resource": "1.3.7",
    "@electerm/electerm-themes": "^1.0.1",
    "@electerm/strip-ansi": "^1.0.0",
    "@fontsource/maple-mono": "^5.2.5",
    "@novnc/novnc": "1.4.0",
    "@types/node": "22.9.3",
    "@vitejs/plugin-react": "4.3.4",
    "@xterm/addon-attach": "0.11.0",
    "@xterm/addon-canvas": "0.7.0",
    "@xterm/addon-fit": "0.10.0",
    "@xterm/addon-ligatures": "0.9.0",
    "@xterm/addon-search": "0.15.0",
    "@xterm/addon-unicode11": "0.8.0",
    "@xterm/addon-web-links": "0.11.0",
    "@xterm/addon-webgl": "0.18.0",
    "@xterm/xterm": "5.5.0",
    "antd": "5.24.9",
    "classnames": "2.5.1",
    "cross-env": "7.0.3",
    "dotenv": "16.3.1",
    "escape-string-regexp": "^5.0.0",
    "express": "4.21.1",
    "express-http-proxy": "^2.0.0",
    "filesize": "10.1.6",
    "filesize-parser": "1.5.1",
    "json-deep-copy": "1.3.1",
    "lodash-es": "^4.17.21",
    "manate": "2.0.0",
    "memoize-one": "6.0.0",
    "morgan": "^1.10.0",
    "nanoid": "3.3.8",
    "pug": "^3.0.2",
    "react": "18.3.1",
    "react-colorful": "^5.6.1",
    "react-delta-hooks": "1.1.5",
    "react-dom": "18.3.1",
    "react-markdown": "9.0.3",
    "replace-in-file": "6.3.5",
    "shelljs": "0.8.5",
    "standard": "^17.1.0",
    "stylus": "^0.64.0",
    "trzsz": "1.1.3",
    "vite": "6.0.1",
    "vite-plugin-commonjs": "0.10.4",
    "vscode-icons": "vscode-icons/vscode-icons",
    "vscode-icons-js": "11.6.1",
    "zmodem-ts": "^1.0.4"
  },
  "standard": {
    "globals": [
      "FakeWs",
      "log",
      "MouseEvent",
      "WebSocket",
      "FileReader",
      "CustomEvent",
      "onmessage",
      "self",
      "localStorage"
    ],
    "ignore": [
      "/public/"
    ],
    "sourceType": "module"
  }
}
